{
  "name": "FieX Api'S",
  "version": "v1.0.0",
  "description": "Simple and easy to use API.",
  "maintenance": {
    "enabled": false,
    "gifUrl": "https://raw.githubusercontent.com/raolbyte/xyz/refs/heads/main/public/maintenance.gif"
  },
  "bannerImage": "/src/banner.jpg",
  "previewImage": "/src/preview.png",
  "header": {
    "status": "Active!"
  },
  "apiSettings": {
    "creator": "FieX Team",
    "apikey": ["fiex-team"]
  },
  "categories": [
    {
      "name": "Artificial Intelligence",
      "items": [
        {
          "name": "LuminAI",
          "desc": "Talk with luminai - Advanced AI assistant",
          "path": "/ai/luminai?text=",
          "status": "ready",
          "params": {
            "text": "Text for luminai to respond to"
          }
        },
        {
          "name": "DeepSeek-GiniGen",
          "desc": "Tanya jawab dengan model DeepSeek (bisa pakai websearch)",
          "path": "/ai/deepseek?text=&websearch=",
          "status": "ready",
          "params": {
            "text": "Pertanyaan yang ingin diajukan",
            "websearch": "(optional) true atau false (default: false)"
          }
        },
        {
          "name": "AIFreeBox Text-to-Image",
          "desc": "Generate gambar AI berdasarkan teks dengan berbagai style (art, peta, thumbnail, kartun)",
          "path": "/ai/aifreebox?prompt=&aspectRatio=&slug=",
          "status": "ready",
          "params": {
            "prompt": "Deskripsi teks gambar yang ingin dibuat",
            "aspectRatio": "(optional) 1:1 | 2:3 | 9:16 | 16:9 (default: 16:9)",
            "slug": "(optional) ai-art-generator | ai-fantasy-map-creator | ai-youtube-thumbnail-generator | ai-old-cartoon-characters-generator"
          }
        },
        {
          "name": "Gemmy AI (Vertex Gemini)",
          "desc": "Gunakan Gemini Vertex AI untuk chat (opsional pakai image/file) atau generate gambar AI dari prompt",
          "path": "/ai/gemmy/:mode?q=&model=&search=",
          "status": "ready",
          "params": {
            "mode": "Mode AI: chat atau image",
            "q": "Teks pertanyaan (chat) atau prompt gambar (image)",
            "model": "(optional) Model Gemini yang digunakan",
            "search": "(optional untuk chat) true/false (aktifkan Google Search)"
          }
        }
      ]
    },
    {
      "name": "Random",
      "items": [
        {
          "name": "Blue Archive",
          "desc": "Get random Blue Archive character images",
          "path": "/random/ba",
          "status": "ready"
        }
      ]
    },
    {
      "name": "Downloader",
      "items": [
        {
          "name": "AIO Downloader",
          "desc": "Download media from all platforms (FB, IG, TikTok, Spotify, dll)",
          "path": "/download/aio?url=",
          "status": "ready",
          "params": {
            "url": "The media URL to download"
          }
        }
      ]
    },
    {
      "name": "Search",
      "items": [
        {
          "name": "YouTube Search",
          "desc": "Search YouTube videos by query",
          "path": "/search/youtube?q=",
          "status": "ready",
          "params": {
            "q": "Search query string"
          }
        }
      ]
    },
    {
      "name": "Tools",
      "items": [
        {
          "name": "TikTok Monetization",
          "desc": "Cek estimasi penghasilan akun TikTok berdasarkan follower dan engagement",
          "path": "/tiktok/monet?username=",
          "status": "ready",
          "params": {
            "username": "Username TikTok tanpa tanda @"
          }
        },
        {
          "name": "YouTube Transcript",
          "desc": "Ambil transkrip dari video YouTube (jika tersedia)",
          "path": "/tools/transcript?url=",
          "status": "ready",
          "params": {
            "url": "URL video YouTube"
          }
        },
        {
          "name": "Image Enhancer (iHancer)",
          "desc": "Tingkatkan kualitas gambar dari URL menggunakan iHancer",
          "path": "/tools/ihancer?url=&size=&method=",
          "status": "ready",
          "params": {
            "url": "URL gambar yang akan ditingkatkan",
            "size": "(optional) low | medium | high (default: medium)",
            "method": "(optional) 1 - 4 (default: 1)"
          }
        }
      ]
    }
  ]
}
